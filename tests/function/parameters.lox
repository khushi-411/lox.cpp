fun f0() { return 0; }
print f0();

fun f1(a) { return a; }
print f1(a);

fun f2(a, b) { return a + b; }
print f2(1, 2);

fun f3(a, b, c) { return a + b + c; }
print f3(1, 2, 3);

fun f4(a, b, c, d) { return a + b + c + d; }
print f4(1, 2, 3, 4);

fun f5(a, b, c, d, e) { return a + b + c + d + e; }
print f5(1, 2, 3, 4, 5);
